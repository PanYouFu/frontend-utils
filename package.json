{
  "name": "frontend-utils",
  "description": "使用rollup构建前端公共方法库",
  "version": "0.0.1",
  "main": "dist/frontend-utils.esm.js",
  "commonjs": "dist/frontend-utils.cjs.js",
  "browser": "dist/frontend-utils.min.js",
  "typings": "./dist/frontend-utils.d.ts",
  "dependencies": {
    "fsevents": "^2.3.3"
  },
  "scripts": {
    "lint": "eslint --ext 'src/**/*.{js,ts}'",
    "lint:fix": "eslint --fix 'src/**/*.{js,ts}'",
    "prettier": "prettier -c --write \"src/**/*\"",
    "test": "jest",
    "build": "npm run clean && rollup -c && api-extractor run --local --verbose",
    "clean": "rimraf dist",
    "doc:generate": "api-documenter markdown -i temp -o doc",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "roots": [
      "src"
    ]
  },
  "author": "PanYouFu",
  "license": "ISC",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.24.6",
    "@microsoft/api-extractor": "^7.43.6",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.8.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rollup": "^4.17.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2"
  },
  "files": [
    "dist",
    "doc",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ]
}
